<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">


    <TextView
        android:id="@+id/licenseName_textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="42dp"
        android:layout_marginTop="53dp"
        android:text="Google Maps API "
        android:textStyle="bold"
        android:textAppearance="@android:style/TextAppearance.Material.Medium" />

    <TextView
        android:id="@+id/textView21"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="URL:"
        android:layout_below="@+id/licenseName_textView2"
        android:layout_alignStart="@+id/licenseName_textView2" />

    <TextView
        android:id="@+id/actualURL_textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/licenseName_textView2"
        android:layout_toEndOf="@+id/textView21"
        android:text="https://developers.google.com/maps/" />
</RelativeLayout>